{
  "background": {"scripts": ["background.js"]},
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Bookmarklet manager"
  },
  "name": "Bookmarklet manager",
  "description": "helps you to keep your bookmarklets and makes it easy to edit them",
  "homepage_url": "http://filype.com/bookmarklet_manage",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png" },
    "permissions": [
      "tabs",
      "bookmarks",
      "http://*/*",
      "https://*/*"
    ],
    "version": "0.1",
    "manifest_version": 2
}
